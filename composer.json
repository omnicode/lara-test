{
	"name": "omnicode/lara-test",
	"description": "Tools for unit testing",
	"keywords": ["laravel", "unit", "testing"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.3.* || 5.4.* || 5.5.*"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"phpunit/phpunit": "~4.0|~5.0"
	},
	"autoload": {
		"psr-4": {
			"LaraTest\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
