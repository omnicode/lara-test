{
	"name": "omnicode/lara-test",
	"description": "Tools for unit testing",
	"keywords": ["laravel", "unit", "testing"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.7.*",
		"omnicode/lara-support": "~4.0"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~7.0"
	},
	"autoload": {
		"psr-4": {
			"LaraTest\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
